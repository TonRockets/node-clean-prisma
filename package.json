{
    "name": "back-end-challenge",
    "version": "1.0.0",
    "description": "> Desafio para os futuros back-end's do [@Personare](https://github.com/Personare)",
    "main": "index.js",
    "scripts": {
        "start": "sucrase-node src/main/server.ts",
        "dev":"ts-node-dev -r tsconfig-paths/register ./src/main/server.ts",
        "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
        "test:staged": "npm test -- findRelatedTests",
        "test:verbose": "jest --passWithNoTests --runInBand",
        "test:unit": "npm test -- --watch -c jest-unit-config.js",
        "test:integration": "npm test -- --watch -c jest-integration-config.js",
        "test:coverage": "jest --passWithNoTests --runInBand --coverage",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts)\"",
        "prepare": "husky install"
    },
    "keywords": [],
    "author": "",
    "license": "GPL-3.0-or-later",
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.1",
        "@types/node": "^20.2.3",
        "@types/supertest": "^2.0.12",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/validator": "^13.7.17",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^34.0.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^15.7.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "lint-staged": "^13.2.3",
        "prettier": "^2.8.8",
        "prisma": "^5.0.0",
        "sucrase": "^3.32.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@prisma/client": "^5.0.0",
        "axios": "^1.4.0",
        "bcrypt": "^5.1.0",
        "express": "^4.18.2",
        "fast-glob": "^3.2.12",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^4.6.3",
        "validator": "^13.9.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
